{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["ES2020"],
    "outDir": "./dist",
    "rootDir": ".",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "types": ["node"],
    "allowSyntheticDefaultImports": true
  },
  "ts-node": {
    "transpileOnly": true,
    "compilerOptions": {
      "module": "commonjs"
    }
  },
  "include": [
    "*.ts"
, "version1.0/analyzeOpenEyeProject.ts", "version2.0/analyzeOpenEyeLifecycle.ts", "version2.0/analyzeOpenEyeLifecycleEnhanced.ts"  ],
  "exclude": [
    "node_modules"
  ]
}
